<p id="notice"><%= notice %></p>

<h1>Listing Area Laborals</h1>

<table>
  <thead>
    <tr>
      <th>Nombre area</th>
      <th>Jefe area</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @area_laborals.each do |area_laboral| %>
      <tr>
        <td><%= area_laboral.nombre_area %></td>
        <td><%= area_laboral.jefe_area %></td>
        <td><%= area_laboral.descripcion %></td>
        <td><%= link_to 'Show', area_laboral %></td>
        <td><%= link_to 'Edit', edit_area_laboral_path(area_laboral) %></td>
        <td><%= link_to 'Destroy', area_laboral, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

