<p id="notice"><%= notice %></p>

<h1>Listing Sucesos</h1>

<table>
  <thead>
    <tr>
      <th>Num evento</th>
      <th>Mensaje</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sucesos.each do |suceso| %>
      <tr>
        <td><%= suceso.num_evento_id %></td>
        <td><%= suceso.mensaje %></td>
        <td><%= link_to 'Show', suceso %></td>
        <td><%= link_to 'Edit', edit_suceso_path(suceso) %></td>
        <td><%= link_to 'Destroy', suceso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Suceso', new_suceso_path %>
