<p id="notice"><%= notice %></p>

<h1>Listing Puesto Laborals</h1>

<table>
  <thead>
    <tr>
      <th>Nombre puesto</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @puesto_laborals.each do |puesto_laboral| %>
      <tr>
        <td><%= puesto_laboral.nombre_puesto %></td>
        <td><%= puesto_laboral.descripcion %></td>
        <td><%= link_to 'Show', puesto_laboral %></td>
        <td><%= link_to 'Edit', edit_puesto_laboral_path(puesto_laboral) %></td>
        <td><%= link_to 'Destroy', puesto_laboral, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Puesto laboral', new_puesto_laboral_path %>
